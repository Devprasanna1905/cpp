#include <stdio.h>
int addnumbers(int a,int b);
int main()
{
    int n,j;
    scanf("%d",&j);
    scanf("%d",&n);
    addnumbers(j,n);
}
int addnumbers(int a,int b)
{
    for(int i=0;i<=b;i++)
    {
       printf("%d\n",a*i);
    }
}
